# Escreva um programa que leia um nÃºmero  inteiro qualquer
# Mostre na tela os n primeiros elementos de uma sequencia de fibonacci
print('-=' * 30)
print('Sequencia de fibonacci')
print("-=" * 30)
numero = int(input("Digite um para ver a sequencia de fibonacci: ")) 
t1 = 0
t2 = 1
contador = 3
print(f"{t1} > {t2}", end = ' ')
while contador <= numero:
    t3 = t1 + t2
    print(f' > {t3}', end = ' ')
    t1 = t2
    t2 = t3 
    contador += 1
    print(f' > Fim ', end = ' ')
print('-=' * 30 )